{% extends "base_app.html.twig" %}
{% block body %}
    <div class="row">
        <div class="row-box col-md-12 text-center score-div center-vertical">
            <div>
                <div class="col-md-6">
                    <span class="score-input">{{ score.note }}/20</span>
                </div>
                <div class="col-md-6">
                    <span class="score-input">{{ (score.note * 100) /20 }} %</span>
                </div>
            </div>
        </div>
    </div>
{% endblock %}