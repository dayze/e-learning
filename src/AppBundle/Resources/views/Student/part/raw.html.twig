<tr>
    <td>{{ student.lastName }}</td>
    <td>{{ student.firstName }}</td>
    <td>{{ student.username }}</td>
    <td>{{ student.email }}</td>
    <td>
        {% for section in student.sections %}
            {% if loop.last %}
                {{ section.promotion|date('Y') }}
            {% endif %}
        {% endfor %}
    </td>
    {% if "ROLE_ADMIN" in app.user.roles %}
    <td>
        <a data-id="{{ student.id }}" href="" class="btn btn-primary btn-xs ajaxEdit"><i class="fa fa-edit"></i></a>
        <a data-id="{{ student.id }}" class="btn btn-danger btn-xs ajaxDelete"><i class="fa fa-remove"></i></a>
    </td>
    {% endif %}
</tr>